<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorPrimary">@color/black</item>
        <item name="colorOnPrimary">@color/black</item>
    </style>

    <style name="TextAppearance" parent="TextAppearance.AppCompat"/>

    <style name="TextAppearance.Type1">
        <item name="fontFamily">@font/inter</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextAppearance.Type2">
        <item name="fontFamily">@font/raleway_500</item>
        <item name="android:fontFamily">@font/raleway_500</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextAppearance.Type3">
        <item name="fontFamily">@font/raleway_700</item>
        <item name="android:fontFamily">@font/raleway_700</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextAppearance.Type4">
        <item name="fontFamily">@font/montserrat</item>
        <item name="android:fontFamily">@font/montserrat</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextAppearance.Type5">
        <item name="fontFamily">@font/raleway_400</item>
        <item name="android:fontFamily">@font/raleway_400</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/purple_100</item>
    </style>

    <style name="TextAppearance.Type6">
        <item name="fontFamily">@font/montserrat</item>
        <item name="android:fontFamily">@font/montserrat</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/orange</item>
    </style>

</resources>